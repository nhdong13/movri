<% if @can_add_to_wish_list %>
  $('.wish-list-btn[data-id=<%= @listing.id %>] span').text("Wish List")
  $('.wish-list-btn[data-id=<%= @listing.id %>]').find(".icon-in-wish-list").removeClass("hidden")
  swal("Successfully!", '<%= @message %>', "success", {buttons: false, timer: 2000});
<% else %>
  swal("Failure!", '<%= @message %>', "error");
<% end %>