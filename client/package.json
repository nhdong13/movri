{
  "repository": "sharetribe/sharetribe",
  "license": "MIT",
  "engines": {
    "node": "12.11.0",
    "npm": "6.11.3"
  },
  "scripts": {
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint .",
    "stylelint": "stylelint **/*.css",
    "build:client": "webpack --mode=production --config webpack.client.config.js",
    "build:server": "webpack --mode=production --config webpack.server.config.js",
    "build:production:client": "NODE_ENV=production webpack --mode=production --config webpack.client.config.js",
    "build:production:server": "NODE_ENV=production webpack --mode=production --config webpack.server.config.js",
    "build:production": "npm run build:production:client && npm run build:production:server",
    "build:dev:client": "webpack --mode=development -w --config webpack.client.config.js",
    "build:dev:server": "webpack --mode=development -w --config webpack.server.config.js",
    "build:test": "npm run build:client && npm run build:server",
    "print-phantomjs-version": "phantomjs --version",
    "start-phantomjs": "phantomjs --webdriver=8910",
    "styleguide": "start-storybook -p 9001",
    "deploy-storybook": "storybook-to-ghpages",
    "test:stories": "mocha './app/components/**/*.story.js' --reporter mocha-circleci-reporter --require @babel/register --require ignore-styles --require ./mochaConfig.js",
    "test:specs": "mocha './app/specs/*.spec.js' --reporter mocha-circleci-reporter --require @babel/register --require ignore-styles --require ./mochaConfig.js",
    "test": "npm run test:stories && npm run test:specs",
    "test:devspecs": "mocha './app/specs/*.spec.js' --require @babel/register",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
